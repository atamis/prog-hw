defvar t = lambda(x) {
  if x then print("pass") else print("fail");
} in {

  t(+(1, 1));
  t(<(0, 0));
  t(<(0, "a"));
  t(<(0, false));
  t(-(10, 10));
  t(+(lambda(x){x}, 4));

  t(+("test", 4));

  x = 10;
  x += 1;

  t(+(45, 43, 1, 3));

  t(==(1, 1));
  t(==('1', 1));
  t(==('1', '1'));

  t(==(lambda(x) { +(x, x) }, lambda(x) {+(x, x)}));
  t(==(lambda(x) {-(x, x)}, lambda(x) {+(x, x)}));
}

